{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "one-var": 0,
    "object-curly-spacing": 0,
    "arrow-parens": 0,
    "eol-last": 0,
    "react/prop-types": 0,
    "brace-style": 0,
    "require-jsdoc": 0,
    "private-props/no-unused-or-undeclared": 2
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "babelOptions": {
      "configFile": ".babelrc"
    }
  },
  "plugins": [
    "private-props"
  ]
}